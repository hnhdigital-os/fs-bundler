{
    "name": "hnhdigital-os/fs-tasker",
    "description": "A Tasker",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "tasker", "bundler", "webpack"],
    "homepage": "https://hnhdigital-os.github.io/fs-tasker",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hnhdigital-os/fs-tasker/issues",
        "source": "https://github.com/hnhdigital-os/fs-tasker"
    },
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "composer/semver": "^3.0",
        "matthiasmullie/minify": "^1.3",
        "scssphp/scssphp": "^1.0",
        "symfony/yaml": "^5.0",
        "laminas/laminas-text": "^2.7"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/fs-tasker"]
}
